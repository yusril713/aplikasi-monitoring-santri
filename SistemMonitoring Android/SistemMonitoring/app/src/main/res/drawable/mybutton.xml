<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="20dp" />
            <stroke android:width="1dip" android:color="#B4F7FF" />
            <gradient android:angle="-90" android:startColor="#B4F7FF" android:endColor="#B4F7FF"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="20dp" />
            <stroke android:width="1dip" android:color="#B4F7FF" />
            <solid android:color="#B4F7FF"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="20dp" />
            <stroke android:width="1dip" android:color="#B4F7FF" />
            <gradient android:angle="-90" android:startColor="#B4F7FF" android:endColor="#EFFAFB" />
        </shape>
    </item>
</selector>